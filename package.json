{
  "name": "CommerceHub",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/connectivity": "^4.3.1",
    "@sap-cloud-sdk/http-client": "^4.3.1",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-ecommercehub": "cds watch --open ecommercehub/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-commerce": "cds watch --open commerce.app.commerce/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "auth": "mocked"
      },
      "destinations": true
    }
  },
  "workspaces": [
    "app/*"
  ]
}
